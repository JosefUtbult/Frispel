  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Django",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}\\site\\manage.py",
      "args": ["runserver", "--pythonpath", "${workspaceFolder}"],
      "django": true,
      "justMyCode": true,
      "env": {
        "DEBUG":"true"
      }
    }
  ]
}